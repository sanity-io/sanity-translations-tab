{
  "name": "sanity-translations-tab",
  "version": "5.0.0",
  "description": "This is the base module for implementing common translation vendor tasks from a Studio, such as sending content to be translated in some specific languages, importing content back etc. Not useful on its own, but vendor-specific plugins will use this for its chrome.",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/sanity-translations-tab#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-translations-tab/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-translations-tab.git"
  },
  "license": "MIT",
  "author": "Sanity.io",
  "contributors": [
    "carolina@sanity.io",
    "tom@sanity.io",
    "annie@all-turtles.com",
    "kkuhnen@gala.games"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "lint": "eslint .",
    "prepare": "husky install",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^3.1.11",
    "@sanity/util": "^5.2.0",
    "sanity-naive-html-serializer": "^4.0.2"
  },
  "devDependencies": {
    "@portabletext/block-tools": "^5.0.0",
    "@portabletext/to-html": "^5.0.1",
    "@sanity/pkg-utils": "^2.4.8",
    "@sanity/plugin-kit": "^3.1.10",
    "@sanity/semantic-release-preset": "^6.0.0",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "rimraf": "^5.0.1",
    "sanity": "^5.2.0",
    "semantic-release": "^25.0.2",
    "styled-components": "^6.1.19",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18.3 || ^19",
    "react-dom": "^18.3 || ^19",
    "sanity": "^3 || ^4 || ^5",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=14"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "packageName": false
    }
  }
}
